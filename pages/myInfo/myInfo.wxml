  <view class="user_base_info">
    <open-data class="userinfo-avatar" type="userAvatarUrl" background-size="cover"></open-data>
    <open-data class="userinfo-nickname" type="userNickName"></open-data>
      <view class="user_info">
        <text>
          姓名：{{userBasicInfo.userName}}
          亲属姓名：{{userBasicInfo.userRelativesName}}
          亲属电话：{{userBasicInfo.userRelativesPhone}}
        </text>
    </view>
  </view>
  <view class="person-list">
    <view class="list-item" bindtap="clickMyClassesInfo">
      <image class="item-image" src="./images/myInfoClass.png"></image>
      <text class="item-text">我的班级</text>
      <view class='arrow'></view>
    </view>
    <view class='divider'></view>
    <view class="list-item" bindtap="clickMyAskForLeave">
      <image class="item-image" src="./images/myInfoCampus.png"></image>
      <text class="item-text">我的请假</text>
      <view class='arrow'></view>
    </view>
    <view class='divider'></view>
    <view class="list-item" bindtap="clickCampusInfo">
      <image class="item-image" src="./images/myInfoCampus.png"></image>
      <text class="item-text">校区信息</text>
      <view class='arrow'></view>
    </view>
    <view class='divider'></view>
    <view class="list-item">
      <image class="item-image" src="./images/myInfoCampus.png"></image>
      <text class="item-text">设置</text>
      <view class='arrow'></view>
    </view>
  </view>
  <mp-tabbar class="tabClass"></mp-tabbar>