<view class="container">
  <view class="userinfo">
    <open-data class="userinfo-avatar" type="userAvatarUrl" background-size="cover"></open-data>
    <open-data class="userinfo-nickname" type="userNickName"></open-data>
  </view>
  <view class="separate"></view>
  <view class="person-list">
    <view class="list-item">
      <image class="item-image" src="./images/myInfoCourse.png"></image>
      <text class="item-text">学习阶段：{{myInfo.userLearnPhase}}</text>
    </view>
    <view class="person-line"></view>
    <view class="list-item">
      <image class="item-image" src="./images/myInfoCourse.png"></image>
      <text class="item-text">课程信息：{{myInfo.courseInfo}}</text>
    </view>
    <view class="person-line"></view>
    <view class="list-item">
      <image class="item-image" src="./images/myInfoTeacher.png"></image>
      <text class="item-text">教师信息：{{myInfo.teacherInfo}}</text>
    </view>
    <view class="person-line"></view>
    <view class="list-item">
      <image class="item-image" src="./images/myInfoClass.png"></image>
      <text class="item-text">班级信息：{{myInfo.classInfo}}</text>
    </view>
    <view class="person-line"></view>
    <view class="list-item">
      <image class="item-image" src="./images/myInfoCampus.png"></image>
      <text class="item-text">校区信息：{{myInfo.campusInfo}}</text>
    </view>
    <view class="person-line"></view>
  </view>
  <view class="separate"></view>
  <mp-tabbar class="tabClass" bindchange="tabChange"></mp-tabbar>
</view>